# Environment Variables for AgriInsure

# Blockchain Configuration
PRIVATE_KEY=your_private_key_here
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_project_id
ETHERSCAN_API_KEY=your_etherscan_api_key

# Backend Configuration
PORT=3001
NODE_ENV=development

# Frontend Configuration
REACT_APP_CONTRACT_ADDRESS=
REACT_APP_NETWORK_ID=11155111
REACT_APP_RPC_URL=https://sepolia.infura.io/v3/your_project_id

# Oracle Configuration
ORACLE_NODES=5
CONSENSUS_THRESHOLD=3

# Weather API Configuration (OpenWeatherMap)
OPENWEATHER_API_KEY=your_openweather_api_key_here
# Get free API key at: https://openweathermap.org/api

# Kaggle Dataset Configuration (optional)
# Absolute or relative path to the Kaggle CSV file (from backend directory)
KAGGLE_DATA_FILE=../datasets/weather.csv
# Column names in the CSV (case-insensitive); adjust to your file
KAGGLE_DATE_COLUMN=date
KAGGLE_RAINFALL_COLUMN=precipitation

# IoT Simulation
SENSOR_UPDATE_INTERVAL=10000
DATA_STORAGE_PATH=./iot-sim/data





